def insertEndDLL(head, data):
    new_node = DLLNode(data)
    if not head:
        return new_node
    temp = head
    while temp.next:
        temp = temp.next
    temp.next = new_node
    new_node.prev = temp
    return head

def deleteAtPosition(head, x):
    if not head:
        return head
    temp = head
    for _ in range(1, x):
        if not temp:
            return head
        temp = temp.next
    if not temp:
        return head
    if temp == head:
        head = temp.next
    if temp.next:
        temp.next.prev = temp.prev
    if temp.prev:
        temp.prev.next = temp.next
    return head

head = None
for val in [1, 2, 3, 4]:
    head = insertEndDLL(head, val)
print("Original DLL:", end=" ")
printDLL(head)
head = deleteAtPosition(head, 3)
print("After Deleting pos 3:", end=" ")
printDLL(head)

