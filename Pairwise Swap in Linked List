def pairwiseSwap(head):
    if not head or not head.next:
        return head
    prev = None
    curr = head
    head = head.next
    while curr and curr.next:
        nxt = curr.next.next
        second = curr.next
        second.next = curr
        curr.next = nxt
        if prev:
            prev.next = second
        prev = curr
        curr = nxt
    return head

def printList(head):
    while head:
        print(head.data, end=" -> ")
        head = head.next
    print("NULL")

head = Node(1)
head.next = Node(2)
head.next.next = Node(3)
head.next.next.next = Node(4)
head.next.next.next.next = Node(5)
head.next.next.next.next.next = Node(6)
print("Original:", end=" ")
printList(head)
head = pairwiseSwap(head)
print("After Swap:", end=" ")
printList(head)
